    "lint:fix": "eslint src/*.{ts, tsx, js, jsx} --fix",
    "lint": "eslint src --color",
    "format": "prettier --write \"src/**/*.{js, jsx, ts, tsx}\" ",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
